# ------------------------------------------------------------------------------
# NODE
# ------------------------------------------------------------------------------

# Load NVM.
export NVM_HOME="$HOME/.nvm"

# ------------------------------------------------------------------------------
# RUBY
# ------------------------------------------------------------------------------

# Load RBENV.
export RBENV_HOME="$HOME/.rbenv"

# ------------------------------------------------------------------------------
# PATH
# ------------------------------------------------------------------------------

export PATH="$NVM_HOME/bin:$RBENV_HOME/bin:/usr/local/bin:/usr/local/sbin:$PATH"
